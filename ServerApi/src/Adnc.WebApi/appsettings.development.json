{
  "Consul": {
    //Consul Client 地址
    "ConsulUrl": "http://localhost:8550",
    //Key路径
    "ConsulKeyPath": "",
    //当前服务名称，可以多个实例共享
    "ServiceName": "andc-api-sys-dev",
    //当前服务地址
    "ServiceUrl": "",
    //服务tag
    "ServerTags": [ "urlprefix-/sys" ],
    //健康检查的地址，当前服务公布出来的一个api接口
    "HealthCheckUrl": "sys/health-24b01005-a76a-4b3b-8fb1-5e0f2e9564fb",
    //心跳间隔
    "HealthCheckIntervalInSecond": 20,
    //是否docker部署服务
    "IsDocker": false
  },
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Mysql": {
    "WriteDbConnectionString": "Server=localhost;Port=13308;database=AlphaNetCore;uid=root;pwd=alpha;"
  },
  "Redis": {
    "MaxRdSecond": 120,
    "EnableLogging": false,
    "LockMs": 5000,
    "SleepMs": 300,
    "dbconfig": {
      "ConnectionStrings": [
        "localhost:13379,password=fbi,defaultDatabase=1,poolsize=50,defaultDatabase=10,prefix="
      ],
      "ReadOnly": false
    }
  },
  "MongoDb": {
    "ConnectionStrings": "mongodb://alpha:football@localhost:13017/Logs",
    "CollectionNamingConvention": 2,
    "PluralizeCollectionNames": true
  },
  "AllowedHosts": "*",
  "CorsHosts": "http://localhost:5001",
  "JWT": {
    "SymmetricSecurityKey": "alphadotnetcoresecurityaabb",
    "Issuer": "http://localhost:5000",
    "ClockSkew": 1,
    "Audience": "",
    "RefreshTokenAudience": "alpha",
    "Expire": 6000,
    "RefreshTokenExpire": 10080
  },
  "SkyWalking": {
    "ServiceName": "adnc-api-sys-dev",
    "Namespace": "",
    "HeaderVersions": [
      "sw6"
    ],
    "Logging": {
      "Level": "Information",
      "FilePath": "logs\\skyapm-{Date}.log"
    },
    "Sampling": {
      "SamplePer3Secs": -1,
      "Percentage": -1.0
    },
    "Transport": {
      "Interval": 3000,
      "ProtocolVersion": "v6",
      "QueueSize": 30000,
      "BatchSize": 3000,
      "gRPC": {
        "Servers": "localhost:11800",
        "Timeout": 10000,
        "ConnectTimeout": 10000,
        "ReportTimeout": 600000
      }
    }
  }
}