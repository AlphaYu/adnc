      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Mysql": {
    "WriteDbConnectionString": "Server=localhost;Port=13308;database=AlphaNetCore;uid=root;pwd=alpha;"
  },
  "Redis": {
    "MaxRdSecond": 120,
    "EnableLogging": false,
