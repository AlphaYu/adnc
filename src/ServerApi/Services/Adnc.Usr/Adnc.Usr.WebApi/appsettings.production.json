{
  "Consul": {
    //Consul Client 地址
    "ConsulUrl": "http://172.16.0.4:8550",
    //Key路径
    "ConsulKeyPath": "adnc/production/usr/appsettings",
    //当前服务名称，可以多个实例共享
    "ServiceName": "adnc.usr.webapi",
    //当前服务地址
    "ServiceUrl": "",
    //服务tag
    "ServerTags": [ "urlprefix-/usr" ],
    //健康检查的地址，当前服务公布出来的一个api接口
    "HealthCheckUrl": "usr/health-24b01005-a76a-4b3b-8fb1-5e0f2e9564fb",
    //心跳间隔
    "HealthCheckIntervalInSecond": 10
  },
  //SkyWalking 只能从json,env配置源获取配置,其他配置源获取不到。
  "SkyWalking": {
    "ServiceName": "adnc.usr.webapi",
    "Namespace": "",
    "HeaderVersions": [
      "sw6"
    ],
    "Logging": {
      "Level": "Error",
      "FilePath": "logs\\skyapm-{Date}.log"
    },
    "Sampling": {
      "SamplePer3Secs": -1,
      "Percentage": -1.0
    },
    "Transport": {
      "Interval": 3000,
      "ProtocolVersion": "v6",
      "QueueSize": 30000,
      "BatchSize": 3000,
      "gRPC": {
        "Servers": "172.16.0.4:11800",
        "Timeout": 10000,
        "ConnectTimeout": 10000,
        "ReportTimeout": 600000
      }
    }
  }
}