<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\..\common.props" />
	<Import Project="..\..\version_infra.props" />
	<Import Project="..\..\nuget.props" />
	<PropertyGroup>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Description>该工程是在`Adnc.Infra.Repository.EfCore`基础上MongoDB的实现。</Description>
		<PackageTags>adnc,efcore,mongodb</PackageTags>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="MongoDB.EntityFrameworkCore" Version="$(MongoDB_EntityFrameworkCore_Version)" />
		<PackageReference Include="DotNetCore.CAP.MySql" Version="$(DotNetCore_CAP_MySql_Version)" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="$(Microsoft_Extensions_Configuration_Binder_Version)" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Adnc.Infra.EfCore\Adnc.Infra.Repository.EfCore.csproj" />
		<ProjectReference Include="..\Adnc.Infra.Repository\Adnc.Infra.Repository.csproj" />
	</ItemGroup>
</Project>