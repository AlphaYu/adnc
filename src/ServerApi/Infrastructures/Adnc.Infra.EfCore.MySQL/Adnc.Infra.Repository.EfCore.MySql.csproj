	<PropertyGroup>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Description>该库是在`Adnc.Infra.Repository.EfCore`基础上Mysql的实现。</Description>
		<PackageTags>adnc,efcore,pomelo</PackageTags>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="SqlGenerator\**" />
