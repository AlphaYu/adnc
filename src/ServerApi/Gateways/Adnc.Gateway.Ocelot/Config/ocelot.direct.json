{
  //direct
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5002"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/usr{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 50010
        }
      ],
      "UpstreamPathTemplate": "/usr{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "LoadBalancerOptions": {
        //LeastConnection,RoundRobin,NoLoadBalance,CookieStickySessions
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/maint{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 50020
        }
      ],
      "UpstreamPathTemplate": "/maint{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/cus{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 50030
        }
      ],
      "UpstreamPathTemplate": "/cus{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ]
}