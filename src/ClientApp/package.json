    "normalize.css": "7.0.0",
    "npm": "^6.14.6",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "vue": "2.6.10",
    "vue-echarts": "^3.1.2",
    "vue-i18n": "7.3.2",
    "vue-router": "3.0.6",
    "vue-table-with-tree-grid": "^0.2.4",
